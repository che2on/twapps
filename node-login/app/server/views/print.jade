
extends layout

block content
	div.container(style='margin:20px')
		table.table.table-bordered.table-striped
			thead
				tr
					th(style='width:40px') #
					th(style='width:180px') Name
					th(style='width:200px') Plan
					th(style='width:180px') Username
					th Account Created
					th User Agent
					th Reply Counter
					th Referral
					th Affiliate
					th Ref ID
					th Aff ID
			tbody
			- for (var i = 0; i < accts.length; i++)
				tr
					td= i+1
					td= accts[i].name
					td= accts[i].country
					td= accts[i].user
					td= accts[i].date
					td= accts[i].useragent
					td= accts[i].replycounter
					td= accts[i].referral
					td= accts[i].affiliate
					td= accts[i].referralid
					td= accts[i].affiliateid